{
  "name": "process-poc",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "bin": {
    "process-poc": "bin/process-poc.js"
  },
  "scripts": {
    "build": "run-s clean",
    "clean": "cross-env-shell \"rm -rf cdk.out coverage dist tmp config.js bin/*.js bin/**/*.js data/**/*.js lib/*.js models/*.js src/**/**/*.js src/**.js stacks/*.js test/*.js\"",
    "cdk": "cdk",
    "cdk-dia": "cdk-dia",
    "check-types": "tsc --pretty --noEmit",
    "create:user": "loadEnvFile=true npm run ts-node bin/developerOperations/devCreateUser/devCreateUser",
    "delete:user": "./bin/developerOperations/devDeleteUser/_devDeleteUser.sh",
    "deploy": "yarn ts-node bin/deploy",
    "deploy:api": "npm run ts-node bin/deployApi",
    "deploy:dynamo": "npm run ts-node bin/deployDynamo",
    "destroy": "npm run ts-node bin/destroy",
    "diff": "npm run cdk diff",
    "generate-diagram": "run-s build synth \"ts-node bin/generateDiagram\"",
    "hotswap": "npm run ts-node bin/hotswap && npm run deploy:api",
    "init-hooks": "chmod +x .githooks/* && ./.githooks/init-hooks.sh",
    "lint": "eslint . --ext .ts",
    "migrate:download": "run-p migrate:download:*",
    "migrate:dynamo": "chmod +x ./data/etl/migrate.sh && ./data/etl/migrate.sh",
    "migrate:upload": "run-p migrate:upload:*",
    "migrate:download:dynamo": "npm run ts-node data/etl/download",
    "migrate:modify:dynamo": "npm run ts-node data/etl/modify",
    "migrate:upload:dynamo": "npm run ts-node data/etl/upload",
    "import:achievements": "npm run ts-node bin/achievements/importAchievementDescriptions",
    "import:dynamo": "run-p import:dynamo:*",
    "import:dynamo:bannedWords": "npm run ts-node bin/loadBannedWords",
    "import:serialCodes": "npm run ts-node bin/serialCodes/importSerialCodeData",
    "pr-check": "run-p lint check-types",
    "synth": "ts-node bin/synth",
    "test": "ts-node bin/test",
    "test:int": "ts-node src/tests/integration/run",
    "ts-node": "ts-node",
    "tsc": "cross-env-shell \"node_modules/.bin/tsc --project tsconfig.build.json\"",
    "watch": "npm run ts-node bin/watch"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.370.0",
    "@aws-sdk/middleware-retry": "^3.374.0",
    "@types/jest": "^29.5.1",
    "@types/node": "20.1.7",
    "aws-cdk": "2.85.0",
    "cdk-assets": "2",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.17",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.85.0",
    "constructs": "^10.0.0",
    "global": "^4.4.0",
    "source-map-support": "^0.5.21"
  }
}
